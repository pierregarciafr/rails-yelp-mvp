<h1>New restaurant</h1>
<%= form_for(@restaurant) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :address %>
<%= f.text_field :address %>
<%= f.label :category %>
<%= f.select :category, collection: Restaurant::CATEGORY %>
<%= f.label :phone_number %>
<%= f.text_field :phone_number %>
<%= f.submit %>
<% end %>
